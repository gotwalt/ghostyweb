<p>Next ghost scheduled for <%=time_ago_in_words(GhostSchedulerWorker.next_at) %> from now.</p>

<table class='table'>
  <tr><th>Time</th><th>Speaker</th><th>Volume</th><th>Track</th><th>Duration</th></tr>
  <% @logs.each do |log| %>
    <tr>
      <td><%=log.created_at %></td>
      <td><%=log.speaker_name %></td>
      <td><%=log.volume %> (<%=log.original_volume%>)</td>
      <td><%=log.audio_uri %></td>
      <td><%=log.duration %></td>
    </tr>
  <% end %>
</table>
